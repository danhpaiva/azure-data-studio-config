{
    "breadcrumbs.enabled": false,
    "editor.fontFamily": "JetBrains Mono, Cascadia Mono",
    "editor.fontSize": 13,
    "editor.letterSpacing": 1.2,
    "editor.lineHeight": 20,
    "editor.fontLigatures": false,
    "editor.tabSize": 2,
    "editor.renderLineHighlight": "gutter",
    "editor.formatOnSave": true,
    "terminal.integrated.cursorStyle": "underline",
    "terminal.integrated.fontFamily": "Cascadia Mono",
    "resultsGrid.fontFamily": "JetBrains Mono, Cascadia Mono",
    "resultsGrid.autoSizeColumns": true,
    "resultsGrid.maxColumnWidth": 200,
    "resultsGrid.cellPadding": [
        5,
        8,
        4
    ],
    "scm.inputFontFamily": "default",
    "terminal.integrated.fontSize": 14,
    "telemetry.telemetryLevel": "off",
    "update.enableWindowsBackgroundUpdates": false,
    "update.mode": "none",
    "update.showReleaseNotes": false,
    "workbench.sideBar.location": "right",
    "workbench.colorTheme": "Default Dark Azure Data Studio",
    "workbench.startupEditor": "none",
    "workbench.activityBar.visible": true,
    "workbench.colorCustomizations": {
        "editor.background": "#191622",
        "editor.foreground": "#E1E1E6",
        "editorBracketHighlight.foreground1": "#E96379"
    },
    "workbench.statusBar.visible": false,
    "window.menuBarVisibility": "compact",
    "workbench.editorAssociations": {
        "*.{sqlplan}": "workbench.editor.executionplan"
    }
}
